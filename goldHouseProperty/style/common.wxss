/* 设置全局背景颜色 */
.bColor{
  background-color: #ff7f02 !important;
}

/* 设置全局字体颜色 */
.fC{
  color: #ff7f02 !important;
}
.fC9{
  color: #999999 !important;
}

/* 提交按钮 */
.submit-btn{
  width: 100%;
  height: 88rpx;
  background-color: #ff7f02;
  font-size: 30rpx;
  color: #fff;
  border-radius: 44rpx;
  display:flex;
  justify-content:center;
  align-items:center;
}
.submit-btn::after{
  border: 0;
}
.submit-btn.disable{
  width: 100%;
  height: 88rpx;
  background-color: #c2c2c2;
  font-size: 30rpx;
  color: #666666;
  display:flex;
  justify-content:center;
  align-items:center;
}
.submit-btn--hover{
  background-color: #e47101;
}

/* 菜单 */
.mine-box{
  position: relative;
}
.mine-box .mine-item{
  min-height: 100rpx;
  padding:10rpx 25rpx;
  position:relative;
  background-color: #fff;
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
  -webkit-box-align:center;
  -webkit-align-items:center;
  align-items:center;
}
.mine-box .mine-item::after{
  content: '';
  position: absolute;
  top: -1px;
  left: 25rpx;
  right: 25rpx;
  z-index: 10;
  border-bottom: 1px solid #f1f1f1;
}

.mine--hover{
  background-color: rgba(0, 0, 0, 0.05) !important;
}

